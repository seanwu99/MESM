	// ****************************************************************************************************************************************************
	function createTableTagComms() {
		// Bind the table rows to this model
		var timestamp = new Date().getTime();
 		oModel.loadData("/XMII/Illuminator?QueryTemplate=MESM/TAGCOMMS/QUERIES/get_TagComms&ts="+timestamp+"&Content-Type=text/xml","",false);
		// alert(oModel.getXML());
		createColumnsTagComms();
		oTable.setModel(oModel);
		oTable.bindRows("/Rowset/Row");
	};

	// ****************************************************************************************************************************************************
	function updateTableTagComms() {
		oTable.clearSelection();
		var timestamp = new Date().getTime();
 		oModel.loadData("/XMII/Illuminator?QueryTemplate=MESM/TAGCOMMS/QUERIES/get_TagComms&ts="+timestamp+"&Content-Type=text/xml","",false);
		oTable.getModel().refresh(true);
	};
