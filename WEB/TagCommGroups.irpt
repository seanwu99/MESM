				var Param4 = escapeSQLString(input_ACTIVE);
				var Param5 = escapeSQLString(input_userNAME);
       				var oReqData;
      				if (window.XMLHttpRequest) {
     					oReqData = new XMLHttpRequest();
      				}
				if (oReqData != null) {
