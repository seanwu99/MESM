			return;
		}
		if(material_name=="") {
			alert("Please Input Material Name ..");
			oCore.byId("TF-EMMaterialName").focus();
			return;
		}
		if(material_description=="") {
			alert("Please Input Material Description ..");
			oCore.byId("TF-EMMaterialDesc").focus();
			return;
		}
