	// ****************************************************************************************************************************************************
	function createTableSqlServers() {
		// Bind the table rows to this model
		var timestamp = new Date().getTime();
 		oModel.loadData("/XMII/Illuminator?QueryTemplate=MESM/SQLSERVERS/QUERIES/get_SqlServers&ts="+timestamp+"&Content-Type=text/xml","",false);
		// alert(oModel.getXML());
		createColumnsSqlServers();
		oTable.setModel(oModel);
		oTable.bindRows("/Rowset/Row");
	};

	// ****************************************************************************************************************************************************
	function updateTableSqlServers() {
		oTable.clearSelection();
		var timestamp = new Date().getTime();
 		oModel.loadData("/XMII/Illuminator?QueryTemplate=MESM/SQLSERVERS/QUERIES/get_SqlServers&ts="+timestamp+"&Content-Type=text/xml","",false);
		oTable.getModel().refresh(true);
	};
