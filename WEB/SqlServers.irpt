	function doTstServer() {
		var status_id = -1;
		var status_text = "";
		var input_NAME = oCore.byId("input_name").getValue();
		var input_DESC = oCore.byId("input_desc").getValue();
		if(input_NAME=="") {
			alert("Please Input Server Name ..");
			oCore.byId("input_name").focus();
		} else {
			if(input_DESC=="") {
				alert("Please Input Server Description ..");
				oCore.byId("input_desc").focus();
			} else {
				// all good. Try to test it ..
				var Param1 = escapeSQLString(input_NAME);
				var Param2 = escapeSQLString(input_DESC);
				var timestamp = new Date().getTime();
				var paramStr = "Param.1="+encodeURIComponent(Param1)+"&Param.2="+encodeURIComponent(Param2);
				getData("/XMII/Illuminator?QueryTemplate=MESM/SQLSERVERS/QUERIES/tst_SqlServer_xAcute&"+paramStr+"&ts="+timestamp+"&Content-Type=text/xml", parseJsonDataSet);
				function parseJsonDataSet(jsondata){
					for (var i=0; i<jsondata.length; i++){
						status_id = jsondata[i]['STATUS'];
						status_text = jsondata[i]['MESSAGE'];
					}
					if(status_id == 0) {
						// SQL Down
						oImage.setSrc("Common/img/red.png");
						oLabel_MSG.setText(" " + status_text);
					} else {
						if (status_id == -1) {
							// SQL Unknown
							oImage.setSrc("Common/img/blue.png");
							oLabel_MSG.setText(" COULD NOT TEST ..");
						} else {
							// SQL Up
							oImage.setSrc("Common/img/green.png");
							oLabel_MSG.setText(" " + status_text);
						}
					}	
				}
			}
		}
	};
