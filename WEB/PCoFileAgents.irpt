	// ****************************************************************************************************************************************************
	function createTablePCoFileAgents() {
		// Bind the table rows to this model
		var timestamp = new Date().getTime();
 		oModel.loadData("/XMII/Illuminator?QueryTemplate=MESM/PCOFILEAGENTS/QUERIES/get_PCoFileAgents&ts="+timestamp+"&Content-Type=text/xml","",false);
		// alert(oModel.getXML());
		createColumnsPCoFileAgents();
		oTable.setModel(oModel);
		oTable.bindRows("/Rowset/Row");
	};

	// ****************************************************************************************************************************************************
	function updateTablePCoFileAgents() {
		oTable.clearSelection();
		var timestamp = new Date().getTime();
 		oModel.loadData("/XMII/Illuminator?QueryTemplate=MESM/PCOFILEAGENTS/QUERIES/get_PCoFileAgents&ts="+timestamp+"&Content-Type=text/xml","",false);
		oTable.getModel().refresh(true);
	};
	// ****************************************************************************************************************************************************
	function createColumnsPCoFileAgents() {
		//Define the columns and the control templates to be used
